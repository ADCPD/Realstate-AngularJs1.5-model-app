<!DOCTYPE html>
<html class="hide-sidebar top-navbar ls-bottom-footer-fixed" lang="{{ locale }}">
<head>
    {% block meta %}
        {% include '@Seo/Seo/indexHome.html.twig' with  { metas : metas } %}
    {% endblock %}

    {% block stylesheets %}
        <link href="{{ asset('bower_components/realestate/css/vendor/all.css') }}" rel="stylesheet">
        <link href="{{ asset('bower_components/realestate/css/app/app.css') }}" rel="stylesheet">

    {% endblock %}
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
   <!-- [end if]-->
</head>
<body>
<!-- Fixed navbar -->
{% include ':realestate:header.html.twig' %}
<div id="content">

    {% block app %}

    {% endblock app %}

</div>
<!-- Footer -->
{% include ':realestate:footer.html.twig' %}
<!-- // Footer -->

{% block javascripts %}
    <!-- TEMPLATE -->
    <script src="https://cdn.jsdelivr.net/jquery/2.2.4/jquery.js"></script>
    <script src="{{ asset('bower_components/realestate/js/vendor/core/bootstrap.js') }}"></script>




    <!-- AngularApp -->

    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>
    <script src="{{ asset('js/fos_js_routes.js') }}"></script>

    <script data-require="angular.js@*"
            src="{{ asset('bundles/interface/js/realstate/app/components/bower_components/angular/angular.js') }}"></script>
    <script src="{{ asset('bundles/interface/js/realstate/app/components/bower_components/angular-route/angular-route.js') }}"></script>
    <script src="{{ asset('bundles/interface/js/realstate/app/components/bower_components/angular-sanitize/angular-sanitize.js') }}"></script>


    <!-- pagination need this librery-->
    <script data-require="ui-bootstrap@*" data-semver="0.12.1"
            src="http://angular-ui.github.io/bootstrap/ui-bootstrap-tpls-0.13.1.min.js"></script>


{% endblock %}
</body>
</html>